<script type="text/javascript">
	$("#code").on("input", function(){
		var code = $("#code").val();
		$.ajax({
			url: '<%=request.getContextPath()%>/auth/public/check-code',
			type: 'POST',
			cache: false,
			data: {
				code: code},
			success: function(data){
				if(data=="1"){
					$(".dataMessCode").html("");
					$("#errorDataCode").html("");
					$("#btnSubmit").removeAttr("disabled");
				}
				if(data=="2"){
					$(".dataMessCode").html("Code không chính xác");
					$("#errorDataCode").html("<p style='color: red; font-style: italic;'>Vui lòng kiểm tra lại code.</p>");
					$("#btnSubmit").attr('disabled', 'disabled');
					
				}
			},
			error: function (){
				alert('Có lỗi xảy ra');
			},
		});
		return false;
	});
